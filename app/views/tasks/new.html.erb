<%= form_for [:list, @task] do |f| %>
  <p><%= f.label :title %></p>
  <p><%= f.text_field :title %></p>
  <p><%= f.label :notes %></p>
  <p><%= f.text_area :notes %></p>
  <p><%= f.label :start_date %></p>
  <p><%= f.date_field :start_date %></p>
  <p><%= f.label :due_date %></p>
  <p><%= f.date_field :due_date %></p>
  <p><%= f.label :status %></p>
  <p><%= f.select :status, ["incomplete", "complete"] %></p>
  <p><%= f.submit %></p>
<% end %>
