<h1>Here are some lists ya dummy</h1>
<% @lists.each do |list| %>
  <h3><%= link_to list.title, list_path(list) %></h3>
  <p><%= list.description %></p>
  <%= link_to "Edit List", edit_list_path(list) %>
  <ul>
  <% list.tasks.each do |task| %>
    <li>
      <p><%=task.title %></p>
      <p><%= task.notes %></p>
      <p><%= task.start_date %></p>
      <p><%= task.due_date %></p>
      <p><%= task.status %></p>
      <h6><%= link_to "Edit Task", edit_list_task_path(list, task) %></h6>
      </li>
    <% end %>
    <h4><%= link_to "Add Task", new_list_task_path(list.id) %></h4>
    </ul>

  <% end %>
  <%= link_to "New List", new_list_path %>
