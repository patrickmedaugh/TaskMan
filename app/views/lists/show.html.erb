<h1><%= @list.title %></h1>
<h3><%= @list.description %></h3>
  <% @list.tasks.each do |task| %>
    <li>
      <p><%=task.title %></p>
      <p><%= task.notes %></p>
      <p><%= task.start_date %></p>
      <p><%= task.due_date %></p>
      <p><%= task.status %></p>
      </li>
    <% end %>
    <p><%= link_to "Add Task", new_list_task_path(@list.id) %>
    <%= link_to "All Lists", lists_path %></p>
    <%= link_to "Edit List", edit_list_path(@list) %>
    <%= link_to "Delete List", list_path(@list), method: :delete %>

